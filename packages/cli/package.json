{
  "name": "@merl/cli",
  "version": "0.4.0",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "description": "A command-line wizard for developer productivity",
  "files": [
    "dist/"
  ],
  "bin": {
    "merl": "dist/cli.js"
  },
  "repository": "https://github.com/chriscasola/merlin.git",
  "author": {
    "name": "Chris Casola",
    "email": "chriscasola@gmail.com"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "prettier --list-different \"./src/**/*.{js,ts}\" && tslint -p \"./tsconfig.json\" && rimraf dist/ && tsc && jest --ci",
    "lint": "prettier --write \"./src/**/*.{js,ts}\" && tslint -p \"./tsconfig.json\" --fix",
    "dev": "jest --watchAll"
  },
  "dependencies": {
    "@merl/git": "file:../git",
    "@merl/web": "file:../web",
    "commander": "^2.17.1",
    "tslib": "^1.9.3"
  }
}
