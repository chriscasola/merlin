{
  "name": "@merl/git",
  "version": "0.3.0",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "description": "A collection of git-related functions for merlin",
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "repository": "https://github.com/chriscasola/merlin.git",
  "author": {
    "name": "Chris Casola",
    "email": "chriscasola@gmail.com"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf dist/ && tsc && jest --ci",
    "lint": "prettier --list-different \"./src/**/*.{js,ts}\" && tslint -p \"./tsconfig.json\"",
    "lint-fix": "prettier --write \"./src/**/*.{js,ts}\" && tslint -p \"./tsconfig.json\" --fix",
    "dev": "jest --watchAll",
    "update-snapshots": "jest -u"
  },
  "dependencies": {
    "@merl/util": "file:../util",
    "tslib": "^1.9.3"
  }
}
